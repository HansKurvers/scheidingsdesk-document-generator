{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": false,
            },
            "enableLiveMetricsFilters": true
        },
        "logLevel": {
            "default": "Debug",
            "Host.Results": "Information",
            "Function": "Debug",
            "Host.Aggregator": "Debug"
        }
    },
    "extensions": {
        "http": {
            "routePrefix": "api",
            "maxConcurrentRequests": 100,
            "customHeaders": {
                "X-Content-Type-Options": "nosniff"
            },
            "cors": {
                "allowedOrigins": [
                    "https://app.scheidingsdesk.nl",
                    "http://localhost:5173",
                    "http://localhost:3000"
                ],
                "supportCredentials": false
            }
        }
    },
    "functionTimeout": "00:05:00"
}